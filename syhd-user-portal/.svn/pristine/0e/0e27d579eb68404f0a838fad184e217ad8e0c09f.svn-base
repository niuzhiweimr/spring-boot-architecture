.my_data {
  .my_site {
    line-height: normal;
  }
  .my_name {
    margin-top: 5px;
  }
}
.my_location {
  font-size: 12px;
}
.my_personage_type {
  position: absolute;
  top: 5px;
  left: 160px;
  z-index: 10;
}
.my_personage_default {
  position: absolute;
  top: 0;
  left: 227px;
  z-index: 10;
  .my_x_content {
    margin: 0;
    padding: 0;
    .my_btn {
      margin: 0;
      padding: 4px 8px;
    }
  }
}
.my_table {
  tbody {
    tr {
      td {
        padding: 8px 8px 5px;
      }
    }
  }
  .my_label {
    span {
      color: #fff;
      font-size: 13px;
      line-height: normal;
      padding: 1px 2px;
      background-color: green;
      border-radius: 2px;
      display: inline-block;
      margin-bottom: 3px;
    }
    .orange {
      background-color: #C85B10;
    }
    .green {
      background-color: green;
    }
    .blue {
      background-color: #2f96b4;
    }
  }
  .my_checkbox {
    height: 16px;
    width: 16px;
    margin: 0;
  }
}